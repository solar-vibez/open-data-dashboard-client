{
  "plugins": [
    "prettier-plugin-css-order",
    "prettier-plugin-tailwindcss"
  ],
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "never",
  "endOfLine": "auto",
  "cssDeclarationSorterOrder": "alphabetical",
  "cssDeclarationSorterKeepOverrides": false
}
